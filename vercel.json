{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "python3.11"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}